{
  "name": "VibeTune - Music Discovery",
  "short_name": "VibeTune",
  "description": "Discover and explore music with VibeTune",
  "start_url": "/dashboard",
  "display": "standalone",
  "background_color": "#0a0f0a",
  "theme_color": "#22c55e",
  "orientation": "portrait",
  "scope": "/",
  "icons": [
    {
      "src": "/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "categories": ["music", "entertainment"],
  "shortcuts": [
    {
      "name": "Search Music",
      "short_name": "Search",
      "description": "Search for songs and artists",
      "url": "/dashboard/search",
      "icons": [{ "src": "/icon-192.png", "sizes": "192x192", "type": "image/png" }]
    },
    {
      "name": "Library",
      "short_name": "Library",
      "description": "View your music library",
      "url": "/dashboard/library",
      "icons": [{ "src": "/icon-192.png", "sizes": "192x192", "type": "image/png" }]
    },
    {
      "name": "Downloaded Songs",
      "short_name": "Downloads",
      "description": "Play your offline music",
      "url": "/dashboard/playlist/downloaded",
      "icons": [{ "src": "/icon-192.png", "sizes": "192x192", "type": "image/png" }]
    },
    {
      "name": "Discover",
      "short_name": "Discover",
      "description": "Find new music recommendations",
      "url": "/dashboard/discover",
      "icons": [{ "src": "/icon-192.png", "sizes": "192x192", "type": "image/png" }]
    },
    {
      "name": "History",
      "short_name": "History",
      "description": "View your listening history",
      "url": "/dashboard/history",
      "icons": [{ "src": "/icon-192.png", "sizes": "192x192", "type": "image/png" }]
    }
  ],
  "share_target": {
    "action": "/dashboard/search",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+music",
      "url": "/dashboard?track=%s"
    }
  ]
}
